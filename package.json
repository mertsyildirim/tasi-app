{
  "name": "tasi-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.x",
    "npm": ">=9.x"
  },
  "scripts": {
    "dev": "next dev",
    "dev:https": "node server.js",
    "dev:secure": "node server-mkcert.js",
    "dev:simple": "node server-simple.js",
    "dev:redirect": "node https-redirect.js",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "test-db": "node src/scripts/test-db.js",
    "seed-db": "node src/scripts/seed-database.js",
    "clean": "if exist .next rmdir /s /q .next && if exist out rmdir /s /q out",
    "build:prod": "npm run clean && SET NODE_ENV=production && next build",
    "export": "next export",
    "deploy": "npm run build:prod && npm start"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "@googlemaps/js-api-loader": "^1.16.8",
    "@heroicons/react": "^2.1.1",
    "@react-google-maps/api": "^2.19.3",
    "@types/google.maps": "^3.58.1",
    "@types/leaflet": "^1.9.17",
    "@types/leaflet-routing-machine": "^3.2.8",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.4.8",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "formidable": "^3.5.2",
    "framer-motion": "^11.0.0",
    "html2canvas": "^1.4.1",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "leaflet": "^1.9.4",
    "leaflet-routing-machine": "^3.2.12",
    "mongoose": "^8.13.2",
    "next": "14.1.3",
    "react": "^18",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18",
    "react-icons": "^5.5.0",
    "react-leaflet": "^4.2.1",
    "serve": "^14.2.4",
    "uuid": "^11.1.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.10.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "https-localhost": "^4.7.1",
    "postcss": "^8",
    "selfsigned": "^2.4.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
